<script>
function _slugify( str ) {
    return str.toString().toLowerCase()
        .replace(/\s+/g, '-')
        .replace(/[^\w\-]+/g, '')
        .replace(/\-\-+/g, '-');
}

function _tocJump(where) {

    var _where = where.split('#')[1];
    var _target_y = jQuery('#' + _where).offset().top;
    jQuery('html,body').animate({ scrollTop: _target_y - 130 }, 350);
    window.location.hash = '#' + _where;
}

function _toc() {

    var _article = document.querySelector("article#post-50509 > main > section");
    var _toc = '<ul class="toc">';
    var _hs = [].slice.call( _article.querySelectorAll('h2, h3:not([class])') );
        _hs.forEach(function(el, i) {

            var _class = 'header-' + el.tagName.toLowerCase();
            var _txt = el.innerText;
            var _id = 'toc-' + _slugify(_txt) + '-' + i;
                el.id = _id; // Set anchor
            if (!!_txt) { // Set menu item
                _toc += '\n';
                _toc += '<li class="' + _class + '"><a href="#' + _id + '" title="' + _txt + '" onclick="_tocJump(this.href); return false;">' + _txt + '</a></li>';
            }
        });

    // Close TOC
    _toc += '\n';
    _toc += '</ul>';
    _toc = (!_hs.length) ? '' : _toc;

    // Inject TOC
